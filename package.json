  "scripts": {
    "clean": "rimraf static",
    "copy:favicon": "cp client/favicon.ico static/favicon.ico",
    "copy:audio": "cp -r client/audio static/",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
